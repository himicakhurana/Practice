		if (m == 0) {
			System.out.println(dict.get(h).toString() + dict.get(0).toString());
		}
		else if(m==15){
			System.out.println("quater past "+dict.get(h).toString());
		} 
		else	if(m<30){
			
		}
		if(m==30){
			System.out.println("half past "+dict.get(h).toString());

		}
		if(m>30&&m!=45){
			System.out.println(dict.get(60-m)+ minutes to
		}

