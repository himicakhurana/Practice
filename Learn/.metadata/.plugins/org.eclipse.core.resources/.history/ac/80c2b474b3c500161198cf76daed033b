public class HammingDistance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		System.out.println(hammingDistance(73, 93));
	}

	public static int hammingDistance(int x, int y) {
		int res = 0;
		System.out.println();
		char[] arr1=Integer.toBinaryString(x).toCharArray();
		char[] arr2=Integer.toBinaryString(y).toCharArray();
		for(int i:arr1)
		System.out.println(Integer.toBinaryString(y));
		res = Math.abs(Integer.toBinaryString(x).compareTo(
				Integer.toBinaryString(y)));
		return res;
	}

}
